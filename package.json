{
  "name": "combined_vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint src/.",
    "prepare": "husky install",
    "lint:staged": "lint-staged --allow-empty"
  },
  "dependencies": {
    "@ghentcdh/vue-component-annotated-text": "0.4.0",
    "@heroicons/vue": "^2.1.5",
    "axios": "^1.7.7",
    "diff": "^5.2.0",
    "express": "^4.21.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.2",
    "tokenizr": "^1.7.0",
    "vue": "^3.5.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tailwindcss/typography": "^0.5.15",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.16.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.12",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "sass": "^1.79.4",
    "sass-loader": "^16.0.2",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.4.5",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.6",
    "vue-tsc": "^2.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint:staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,vue,mts}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
